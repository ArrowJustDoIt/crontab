{
    "name": "arrowjustdoit/crontab",
    "description": "Crontab extension for laravel-admin",
    "type": "library",
    "keywords": ["laravel-admin", "extension", "crontab"],
    "homepage": "https://github.com/ArrowJustDoIt/crontab",
    "license": "MIT",
    "authors": [
        {
            "name": "arrow",
            "email": "arrowylq@outlook.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "encore/laravel-admin": "~1.6",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "ArrowJustDoIt\\Crontab\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ArrowJustDoIt\\Crontab\\CrontabServiceProvider"
            ]

        }
    }
